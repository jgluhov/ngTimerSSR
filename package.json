{
  "name": "ng-counter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && yarn ng run ngTimerSSR:server",
    "build:ssr": "yarn run build:client-and-server-bundles && yarn run webpack:server && yarn run prerender:routes",
    "serve:ssr": "node ./dist/server.js",
    "watch:ssr": "watch 'yarn kill && yarn run build\\:ssr && yarn run serve\\:ssr' ./src/app",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prerender:routes": "node ./dist/prerenderer.js",
    "kill": "npx kill-port 3000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.6",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "^7.2.6",
    "@angular/router": "~7.2.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "core-js": "^2.5.4",
    "global": "^4.3.2",
    "mkdirp": "^0.5.1",
    "rxjs": "~6.4.0",
    "shelljs": "^0.8.3",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.0",
    "watch": "^1.0.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.9.5",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.13.0"
  }
}
